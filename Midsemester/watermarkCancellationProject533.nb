Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"solveXYZ", "[", 
    RowBox[{"a_", ",", "b_", ",", "c_", ",", "t_"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", "}"}], ",", 
     RowBox[{
      RowBox[{"mat", "=", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{
           RowBox[{"Tr", "[", 
            RowBox[{"a", ".", 
             RowBox[{"Transpose", "[", "a", "]"}]}], "]"}], ",", 
           RowBox[{"Tr", "[", 
            RowBox[{"b", ".", 
             RowBox[{"Transpose", "[", "a", "]"}]}], "]"}], ",", 
           RowBox[{"Tr", "[", 
            RowBox[{"c", ".", 
             RowBox[{"Transpose", "[", "a", "]"}]}], "]"}]}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"Tr", "[", 
            RowBox[{"a", ".", 
             RowBox[{"Transpose", "[", "b", "]"}]}], "]"}], ",", 
           RowBox[{"Tr", "[", 
            RowBox[{"b", ".", 
             RowBox[{"Transpose", "[", "b", "]"}]}], "]"}], ",", 
           RowBox[{"Tr", "[", 
            RowBox[{"c", ".", 
             RowBox[{"Transpose", "[", "b", "]"}]}], "]"}]}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"Tr", "[", 
            RowBox[{"a", ".", 
             RowBox[{"Transpose", "[", "c", "]"}]}], "]"}], ",", 
           RowBox[{"Tr", "[", 
            RowBox[{"b", ".", 
             RowBox[{"Transpose", "[", "c", "]"}]}], "]"}], ",", 
           RowBox[{"Tr", "[", 
            RowBox[{"c", ".", 
             RowBox[{"Transpose", "[", "c", "]"}]}], "]"}]}], "}"}]}], 
        "}"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"vec", "=", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"Tr", "[", 
          RowBox[{"t", ".", 
           RowBox[{"Transpose", "[", "a", "]"}]}], "]"}], ",", 
         RowBox[{"Tr", "[", 
          RowBox[{"t", ".", 
           RowBox[{"Transpose", "[", "b", "]"}]}], "]"}], ",", 
         RowBox[{"Tr", "[", 
          RowBox[{"t", ".", 
           RowBox[{"Transpose", "[", "c", "]"}]}], "]"}]}], "}"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"LinearSolve", "[", 
       RowBox[{"mat", ",", "vec"}], "]"}]}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"{", 
   RowBox[{"xstar3", ",", "ystar3", ",", "temp"}], "}"}], "=", 
  RowBox[{"solveXYZ", "[", 
   RowBox[{"a", ",", "b", ",", 
    RowBox[{"ConstantArray", "[", 
     RowBox[{"1", ",", 
      RowBox[{"Dimensions", "[", "a", "]"}]}], "]"}], ",", "t"}], 
   "]"}]}]}], "Input",
