 RowBox[{
  RowBox[{"de1", "=", 
   RowBox[{
    SubscriptBox["\[PartialD]", "x"], 
    FractionBox[
     RowBox[{
      RowBox[{"Sin", "[", "x", "]"}], " ", 
      RowBox[{"Cos", "[", "x", "]"}]}], 
     RowBox[{"1", "+", 
      SuperscriptBox["x", "2"]}]]}]}], ";", 
  RowBox[{"p2", "=", 
   RowBox[{"Plot", "[", 
    RowBox[{"de1", ",", 
