Cell["Note the syntax of If command is as follows.", "Text",ExpressionUUID->"a4d8908b-9705-4c65-be8c-1a783a9ecd6b"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"If", "[", 
    RowBox[{"test", ",", " ", 
     RowBox[{
     "Statements", " ", "to", " ", "be", " ", "executed", " ", "if", " ", 
      "test", " ", "is", " ", "True"}], ",", 
     RowBox[{
     "Statements", " ", "to", " ", "be", " ", "executed", " ", "if", " ", 
      "test", " ", "is", " ", "False"}]}], "]"}], TraditionalForm]],
  ExpressionUUID->"e5432d8b-c24d-46ae-8257-b2b342639bb9"]
}], "Text",ExpressionUUID->"be770bbb-1474-46f4-9a08-9b4ae892d88c"]
