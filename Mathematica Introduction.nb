 FormBox[
  RowBox[{
   RowBox[{"Sin", "::", "\<\"argx\"\>"}], ":", 
   " ", "\<\"\\!\\(TraditionalForm\\`Sin\\) called with \\!\\(TraditionalForm\
\\`4\\) arguments; 1 argument is expected.\"\>"}], 
  TraditionalForm]], "Message",ExpressionUUID->"114e12c2-fe15-440d-a0cd-\
11a3129ffaac"],

