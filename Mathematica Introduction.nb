   RowBox[{
    FractionBox["21", "20"], ",", 
    RowBox[{"-", 
     FractionBox["13", "20"]}], ",", 
    FractionBox["1", "4"]}], "}"}], TraditionalForm]], "Output",ExpressionUUID\
->"81944e83-e1b6-4ba7-b3db-fa17e993cdb5"]
