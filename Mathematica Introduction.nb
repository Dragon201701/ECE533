Cell[BoxData[
 FormBox[
  RowBox[{"(", "\[NoBreak]", GridBox[{
     {
      RowBox[{"x", " ", "y"}], 
      SuperscriptBox["x", "2"]},
     {
      RowBox[{"-", 
       FractionBox[
        RowBox[{"3", " ", "y"}], 
