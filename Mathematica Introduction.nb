  RowBox[{"thickCylinderStresses", "[", 
   RowBox[{"20", ",", "ri", ",", "15", ",", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{"ri", "+", "15"}], ")"}], "/", "2"}]}], "]"}]}]], "Input",Expres\
sionUUID->"6c8d1466-d46a-40df-a82f-04bbb834666b"],

